                                                $scope.CustomerOrderModel.UpdateSpecialOrder(successfulSearchResult.specialOrderList);
                                            $scope.bindCheckOutList(successfulSearchResult.coInvoiceItemRecs);
                                            if(refreshEditMode){
                                                $scope.CustomerOrderModel.createGridEditItem(soIndex);


                                            }
                                        },
