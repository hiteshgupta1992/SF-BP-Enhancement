                // Info Header
                
                $scope.VendorOrderModel.LeftSideHeadingLables['Info'] = String.fromCharCode(currentHeadingSequenceIndex++);
                
                $scope.VendorOrderModel.LeftSideHeadingLables['Order'] = String.fromCharCode(currentHeadingSequenceIndex++);
                
