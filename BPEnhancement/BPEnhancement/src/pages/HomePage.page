<script type="text/javascript">
    var homePageModule= angular.module('homePageModule', ['BPGlobalHeader','materialDatePicker', 'ui.sortable'] );
            homePageModule.controller('homePageCtrl', function ($scope, HomePageService,$location) {
             $scope.HomePage = {};
             $scope.HomePage.widgetModel={};
             $scope.HomePage.isShow=true;
            
             $scope.header = {
                monday: 'M',
                tuesday: 'T',
                wednesday: 'W',
                thursday: 'T',
                friday: 'F',
                saturday: 'S',
                sunday: 'S',
         }
            $scope.HomePage.showSidePanel=function(){
                 $scope.absUrl = $location.absUrl();
                 debug($scope.absUrl)  ;
                $scope.HomePage.isShow=false;
           }
                
           $scope.HomePage.hideSidePanel=function(){
               $scope.HomePage.isShow=true;
                    
           }
           
           $scope.HomePage.LoadWidget = function(){
              HomePageService.getWidgetRecord()
                   .then(function (widgetInfo) {
	            $scope.HomePage.widgetModel = widgetInfo;
	            
            	HomePageService.CustomerOrderWidgetUpdate()
                   .then(function (CustomerInfo) {
		            $scope.HomePage.HomepageData = CustomerInfo;
					debug(CustomerInfo);
				}, function (errorSearchResult) {
				    responseData = errorSearchResult;
				});  
				
	          debug( $scope.HomePage.widgetModel);
	          }, function (errorSearchResult) {
	              responseData = errorSearchResult;
	          });
          }
          // Drag-drop configurations for grid columns
        $scope.HomePage.insideSortable = {
            items: '.draggable',
            cursor : 'move',
            tolerance : 'pointer',
            connectWith : '.insideDroppable1',
            start : function(event, ui) {
            },
            over : function(event, ui) {
            },
            out : function(event, ui) {
            },
            change : function(event, ui) {
            },
            stop : function(event, ui) {
               HomePageService.widgetOrderUpdate(JSON.stringify($scope.HomePage.widgetModel))
                    .then(function (updatedWidgetInfo) {
                        debug( updatedWidgetInfo);
                    }, function (errorSearchResult) {
                        responseData = errorSearchResult;
               });
            }
        };
        
        $scope.HomePage.addRemoveWidget= function(widgetId,isVisible){
            debug(widgetId);
            debug(isVisible);
         HomePageService.showHideWidget(widgetId, isVisible)
               .then(function (updatedWidgetInfo) {
                $scope.HomePage.widgetModel = updatedWidgetInfo;
               }, function (errorSearchResult) {
                   responseData = errorSearchResult;
         });
        }
        
        $scope.HomePage.addWidget = function(addWidgetId){
            var isVisible;
            for(var i=0;i<$scope.HomePage.widgetModel.length; i++){
                if($scope.HomePage.widgetModel[i].WidgetName == addWidgetId)
                {
                    addWidgetId = $scope.HomePage.widgetModel[i].Id;
                    isVisible = $scope.HomePage.widgetModel[i].IsVisible;
                }
            }
            debug(isVisible );
            if(isVisible == false){
                 $scope.HomePage.addRemoveWidget(addWidgetId, !isVisible);
            }
        }
        
        
        $scope.HomePage.SearchSelling = function(event){
	        var code = (event.keyCode ? event.keyCode : event.which);
			if(code == 13) { //Enter keycode
			    window.open("{!$Page.HomeSearch}?q="+$scope.HomePage.sellingSearchText+"&type=Customer Orders&owner=0","_self");
			}
        
        }
        
        
        $scope.HomePage.SearchOrdering = function(event){
        	var code = (event.keyCode ? event.keyCode : event.which);
			if(code == 13) { //Enter keycode
			    window.open("{!$Page.HomeSearch}?q="+$scope.HomePage.orderingSearchText+"&type=Vendor Orders&owner=0","_self");
			}
        }
        
        
        $scope.HomePage.navigateTo = function(linkId){
		        if(linkId == 'Home'){
		        	window.open("{!$Page.Homepage}","_self");
		        }else if(linkId == 'Selling'){
		        	window.open("{!$Page.HomeSearch}?type=Customer Orders","_self");
		        }else if(linkId == 'Ordering'){
		        	window.open("{!$Page.HomeSearch}?type=Vendor Orders","_self");
		        }else if(linkId == 'Actions'){
		        
		        }else if(linkId == 'Alerts'){
		        
		        }else if(linkId == 'Tasks'){
		        
		        }else if(linkId == 'Chatter'){
		        
		        }else if(linkId == 'Search'){
		        	window.open("{!$Page.HomeSearch}","_self");
		        }else if(linkId == 'News'){
		        
		        }else if(linkId == 'Dashboards'){
		        
		        }else if(linkId == 'Report'){
		        
		        }else if(linkId == 'Integate'){
		        
		        }else if(linkId == 'My_Profile'){
		        	window.open("/{!$user.Id}","_self");
		        }else if(linkId == 'Setting'){
		        	window.open("{!$Page.BlackPurlHome}","_self");
		        }
	        
	        }
        
        
       });
         homePageModule.service("HomePageService", function ($q) {
                    this.getWidgetRecord = function () {
                        var deferred = $q.defer();
        
                        // make AJAX request to the remote service
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.WidgetCtrl.getWidgetRecord}',
                            function (result, event) {
                            if (event.type == 'exception') {
                                deferred.reject(event.message);
                            } else {
                                var widgetDetails = decodeString(result);
                                var find = '\'';
                                var re = new RegExp(find, 'g');
                                widgetDetails = widgetDetails.replace(re, '');
                                deferred.resolve(JSON.parse(widgetDetails));
                            }
                        }, {
                            escape : true
                        });
                        return deferred.promise;
                    },
                    
                    this.CustomerOrderWidgetUpdate = function () {
                        var deferred = $q.defer();
        
                        // make AJAX request to the remote service
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.WidgetCtrl.getCustomerOrderWidgetUpdate}',
                            function (result, event) {
                            if (event.type == 'exception') {
                                deferred.reject(event.message);
                            } else {
                                var widgetDetails = decodeString(result);
                                var find = '\'';
                                var re = new RegExp(find, 'g');
                                widgetDetails = widgetDetails.replace(re, '');
                                deferred.resolve(JSON.parse(widgetDetails));
                            }
                        }, {
                            escape : true
                        });
                        return deferred.promise;
                    },
                    
                     this.widgetOrderUpdate = function (updatedWidgetList) {
                     	updatedWidgetList = encodeString(updatedWidgetList);
                        var deferred = $q.defer();
        
                        // make AJAX request to the remote service
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.WidgetCtrl.widgetOrderUpdate}',updatedWidgetList,
                            function (result, event) {
                            if (event.type == 'exception') {
                                deferred.reject(event.message);
                            } else {
                                var updatedWidgetDetails = decodeString(result);
                                var find = '\'';
                                var re = new RegExp(find, 'g');
                                updatedWidgetDetails = updatedWidgetDetails.replace(re, '');
                                deferred.resolve(JSON.parse(updatedWidgetDetails));
                            }
                        }, {
                            escape : true
                        });
                        return deferred.promise;
                    },
                    
                      this.showHideWidget = function (widgetId, isvisible) {
                      	widgetId = encodeString(widgetId);
                        var deferred = $q.defer();
        
                        // make AJAX request to the remote service
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.WidgetCtrl.showHideWidget}',widgetId,isvisible,
                            function (result, event) {
                            if (event.type == 'exception') {
                                deferred.reject(event.message);
                            } else {
                                var updatedWidgetDetails = decodeString(result);
                                var find = '\'';
                                var re = new RegExp(find, 'g');
                                updatedWidgetDetails = updatedWidgetDetails.replace(re, '');
                                deferred.resolve(JSON.parse(updatedWidgetDetails));
                            }
                        }, {
                            escape : true
                        });
                        return deferred.promise;
                    }
                    
                    
                    
           })
</script>

