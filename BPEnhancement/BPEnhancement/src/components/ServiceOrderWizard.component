                                       <input type="text" class="mytxtbox form-control" id="txtconcern" ng-model="CustomerOrderModel.SOHeaderList[WizardModel.WizardSOHeaderIndex].SOHeaderInfo.NewCorrection" 
                                           ng-attr-tabindex="{{CustomerOrderModel.tabIndexValue + (WizardModel.WizardSOHeaderIndex * 12) + 3}}" 
                                           placeholder="Add Item" rel="Concern" ng-keydown="CustomerOrderModel.addAndRemoveFromMultiSelect($event, WizardModel.WizardSOHeaderIndex, 'NewCorrection', 'ManualCorrection', 'Correction');" />
                                   </div>
