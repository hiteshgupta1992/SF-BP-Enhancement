											<col width="18%"  />
											<col width="18%"  />
											<thead>
												<tr class="tableHeader T3 C2">
													<th>ITEM/DESC</th>
													<th class="text-center">QUANTITY</th>
													<th class="text-center">PRICE</th>
													<th class="text-center">TOTAL</th>
													<th class="text-center">BILL TO</th>
													<th class="text-center">CATEGORY</th>
												</tr>
											</thead>
											<tbody ng-repeat="VarianceLineItem in WizardModel.wizardInfo.VarianceLineItemList">
												<tr class="clickable_row" id="row{{$index}}" ng-class-even="'C2'" 
													ng-class="{'disabledClaimLineItemRow': WizardModel.WizardSOHeader.SOHeaderInfo.ClaimStatus =='Approved'}">
													<td id="VarianceLineItemsToApprove_crWiz_grid_container_tbody_tr_td_1_{{$index}}" class="T7 tdPadding20" >
														{{VarianceLineItem.Itemdesc}}
													</td>
													<td id="VarianceLineItemsToApprove_crWiz_grid_container_tbody_tr_td_2_{{$index}}" class="T7 text-center" 
														ng-class="{'Requiredtd': VarianceLineItem.VarianceQuantity < 0}">
														{{VarianceLineItem.VarianceQuantity}}
													</td>
													<td id="VarianceLineItemsToApprove_crWiz_grid_container_tbody_tr_td_3_{{$index}}" class="T7 text-center" 
														ng-class="{'Requiredtd': VarianceLineItem.VarianceAmount < 0}">
														{{VarianceLineItem.VarianceAmount | currency}}
													</td>
													<td id="VarianceLineItemsToApprove_crWiz_grid_container_tbody_tr_td_4_{{$index}}" class="T7 text-center" 
														ng-class="{'Requiredtd': VarianceLineItem.Variance < 0}">
														<b>{{VarianceLineItem.TotalVariance | currency}}</b>
													</td>
													<td id="VarianceLineItemsToApprove_crWiz_grid_container_tbody_tr_td_5_{{$index}}" class="T7 text-center" >
														<span class="select_box_container" ng-if="VarianceLineItem.VarianceQuantity != 0">
															<select class="form-control"
												               ng-attr-tabindex="1010"
												               id="VarianceLineBillTo_crWiz{{$index}}"
												               ng-options="item for item in WizardModel.wizardInfo.VarianceBillToList"
												               ng-model="VarianceLineItem.BillTo"
												               ng-change="CustomerOrderModel.changeVarianceBillTo($index)" >
												            </select>
														</span>
														<span ng-if="VarianceLineItem.VarianceQuantity == 0">
															{{VarianceLineItem.BillTo}}
														</span>
													</td>
													<td id="VarianceLineItemsToApprove_crWiz_grid_container_tbody_tr_td_6_{{$index}}" class="T7 text-center" >
														<span class="select_box_container" 
															ng-if="VarianceLineItem.BillTo != 'Customer' && (VarianceLineItem.BillTo == 'Internal' || VarianceLineItem.VarianceQuantity == 0 || (VarianceLineItem.VarianceQuantity != 0 && VarianceLineItem.VarianceAmount != 0))">
															<select class="form-control"
												               ng-attr-tabindex="1010"
												               id="VarianceLineCategory_crWiz{{$index}}"
												               ng-options="item.Id as item.CategoryName for item in WizardModel.wizardInfo.InternalCategoryList"
												               ng-model="VarianceLineItem.Category" 
												               ng-change="CustomerOrderModel.changeVarianceCategory($index)" >
												            </select>
														</span>
													</td>
												</tr>
											</tbody>
										</table>
									</div>
								</div>
								<!-- END: Variance Disposition items grid -->
	        				</div>
	        			</div>
	        			<!-- START: Respnse Detail wizard step section -->
	        		</div>
	        		<!-- END: Claim Response Wizard -->
                      
					<!-- START: View response Wizard -->
	        		<div ng-if="WizardModel.currentClaimWizardKey == 'View_Claim_Response'">
	        			
	        		</div>
	        		<!-- END: View response Wizard -->
               </div>
            </div>
            <div class="buttonSection row" style="clear: both;">
               
               <div class="col-md-12 buttonsDiv">
                  <div class="pull-right">
                  	<div style="float: left; margin: 4px 10px; color: #c9302c;" 
                  			ng-if="WizardModel.currentClaimWizardKey == 'Claim_Response' && WizardModel.getCurrentActiveChevronIndex() == 2 && WizardModel.wizardInfo.VarianceHeader != null && WizardModel.wizardInfo.VarianceHeader.length != 0"> 
                  				{{WizardModel.wizardInfo.VarianceHeader}}
                  	</div>
                  	
                    <button class="btn C11 T4 addmoreinfoBtn pull-left pushBtn"
                        ng-click="$scope.WizardModel.saveServiceClaimWizardData(); WizardModel.SendEmailOnClaimWizard();"
                        ng-if="CustomerOrderModel.showSection('Submit_Claim')">Send</button>
                     
                     <button class="btn C11 T4 addmoreinfoBtn pull-left"
                        ng-if="WizardModel.currentClaimWizardKey == 'Claim_Submission' && CustomerOrderModel.isNextbuttonActive()"
                        ng-click="CustomerOrderModel.NextAction(-1)">Next</button>
                     
                     <button class="btn C11 T4 addmoreinfoBtn pull-left"
                        ng-if="WizardModel.currentClaimWizardKey == 'Claim_Response' && WizardModel.getCurrentActiveChevronIndex() != 3 && WizardModel.wizardInfo.NextButtonAvail"
                        ng-click="CustomerOrderModel.NextAction(-1)">Next</button>
                     <button class="btn C11 T4 addmoreinfoBtn pull-left pushBtn" 
                    		ng-if="WizardModel.currentClaimWizardKey == 'Claim_Response' && WizardModel.getCurrentActiveChevronIndex() == 3 && WizardModel.wizardInfo.FinalizeButtonAvail"
                    		ng-click="CustomerOrderModel.SaveAndCloseClaimResponseAction(-1)">Finalize</button>
                        
                     <div class="dropdown pull-left dropup" style="margin: 4px 10px;">
                        <a href="#" data-toggle="dropdown" class="dropdown-toggle">Save and Exit <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                        	<li>
                           		<a href="#" ng-click="WizardModel.saveAndExitSignIn(true);"
                           					ng-if="WizardModel.currentClaimWizardKey == 'Claim_Submission'">Save and Exit</a>
                           	
                           		<a href="#" ng-click="CustomerOrderModel.SaveAndCloseClaimResponseAction(-1);"
                           					ng-if="WizardModel.currentClaimWizardKey == 'Claim_Response'">Save and Exit</a>
                           	</li>
                           <li><a href="#" ng-click="WizardModel.closeWizardMode();">Cancel</a></li>
                        </ul>
                     </div>
                  </div>
               </div>
            </div>
        </div>
</div>
</apex:component>