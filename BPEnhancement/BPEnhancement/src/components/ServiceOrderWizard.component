    <div class="wizardWrapper col-md-12 P0" ng-if="CustomerOrderModel.isWizardMode">
    
        <div ng-if="CustomerOrderModel.isWorkWizardMode">
        
            <div class="buttonSection row">
               <div class="col-md-5">
                  <h1 class="main_heading" ng-if="WizardModel.currentWizardKey == 'Sign_In'">Service Sign-in Wizard</h1>
                  <h1 class="main_heading" ng-if="WizardModel.currentWizardKey == 'Service_Sign_Out'">Sign-out Wizard</h1>
                  <h1 class="main_heading" ng-if="WizardModel.currentWizardKey == 'Review_Service'">Review Service Wizard</h1>
                  <h1 class="main_heading" ng-if="WizardModel.currentWizardKey == 'Get_Customer_Approval'">Get Customer Approval</h1>
                  
               </div>
               <div class="col-md-7 buttonsDiv">
                  <div class="pull-right">
                     <div class="dpInline" ng-if="WizardModel.currentWizardKey == 'Sign_In' || WizardModel.currentWizardKey == 'Service_Sign_Out'
                     							|| WizardModel.currentWizardKey == 'Get_Customer_Approval'">
                         <button class="btn C11 T4 addmoreinfoBtn pull-left pushBtn"
                            ng-if="CustomerOrderModel.showSection('Digital_Signature_Approval') && WizardModel.wizardInfo.Sign_In.Digital_Signature_Approval">Push</button>
                         <button class="btn C11 T4 addmoreinfoBtn pull-left pushBtn"
                            ng-click="CustomerOrderModel.PrintPriview(WizardModel.WizardSOHeaderId)" 
                            ng-if="CustomerOrderModel.showSection('Print_Approval') && WizardModel.wizardInfo.Sign_In.Print_Approval">Print</button>
                        <button class="btn C11 T4 addmoreinfoBtn pull-left pushBtn"
                            ng-click="WizardModel.saveWizardData(); WizardModel.SendEmailOnSignInWizard()"
                            ng-if="CustomerOrderModel.showSection('Remotely_Approval') && WizardModel.wizardInfo.Sign_In.Remotely_Approval">Send</button>
                     </div>
                     
                     <div class="dpInline" ng-if="WizardModel.currentWizardKey == 'Review_Service'">
                        <button class="btn C11 T4 addmoreinfoBtn pull-left pushBtn"
                        	ng-click="WizardModel.submitForReview();">Submit</button>
                     </div>
                     
                     <button class="btn C11 T4 addmoreinfoBtn pull-left"
                        ng-if="CustomerOrderModel.isNextbuttonActive()"
                        ng-click="CustomerOrderModel.NextAction(-1)">Next</button>
                     <div class="dropdown pull-left" style="margin: 4px 10px;">
                        <a href="#" data-toggle="dropdown" class="dropdown-toggle">Save and Exit <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                           <li><a href="#" ng-click="WizardModel.saveAndExitSignIn(true);">Save and Exit</a></li>
                           <li><a href="#" ng-click="WizardModel.closeWizardMode();">Cancel</a></li>
                        </ul>
                     </div>
                  </div>
               </div>
            </div>
            <div id="crumbs">
               <ul>
                  <li ng-repeat="chevron in CustomerOrderModel.currentChevronList">
                     <a href="#" ng-class="{'activeChevron' : chevron.isActive}"
                        ng-click="CustomerOrderModel.NextAction($index)"> 
                     <i class="fa fa-circle-o" aria-hidden="true"></i>{{chevron.Name}}
                     </a>  
                  </li>
               </ul>
            </div>
