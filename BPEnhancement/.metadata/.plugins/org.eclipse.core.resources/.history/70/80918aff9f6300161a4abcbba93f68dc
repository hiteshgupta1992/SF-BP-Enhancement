            $(document).ready(function () {
                $('.Customer-info-overlay').mouseover(function() {
                    $('.Customer-info-overlay').show(); 
                });
                $('.Customer-info-overlay').mouseout(function() {
                    $('.Customer-info-overlay').hide(); 
                });
                $('#Customer-info-flyout .closeBtn').click(function() {
                    $('.Customer-info-overlay').hide(); 
                });
            });
